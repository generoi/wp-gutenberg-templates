{
  "name": "generoi/wp-gutenberg-templates",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "Add page template support to Gutenberg",
  "homepage": "https://github.com/generoi/wp-gutenberg-templates",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi" }
  ],
  "keywords": [
    "wordpress",
    "gutenberg"
  ],
  "support": {
    "issues": "https://github.com/generoi/wp-gutenberg-templates/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.minasithil.genero.fi/"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "brick/varexporter": "^0.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan": "^1.8",
    "szepeviktor/phpstan-wordpress": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "GeneroWP\\Gutenberg\\Templates\\": "src/"
    },
    "files": [
      "src/api.php"
    ]
  },
  "scripts": {
    "test": [
      "phpcs",
      "phpstan"
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  }
}
